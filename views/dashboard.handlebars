<body id="profile-body">
        <div id="profile-img-container">
            <img src="{{profileImage}}" id="profile-img">       
        </div>
        <div class="row" id="profile-page-header">
            <h1 id="profile-header-background-type">s0cial3r</h1>
        </div>
        <nav class="row" id="profile-nav">
                <a href="/dashboard" id="profile-logo">0</a>
            <span id="username-display">{{displayName}}</span>
            <ul id="nav-mobile" class="right hide-on-med-and-down chat-nav-links">
                <li><a href="#" class="nav-links highlighted" id="main-chat-link">Main Chat</a></li>
                <li><a href="#" class="nav-links" id="gaming-chat-link">Gaming Chat</a></li>
                <li><a href="#" class="nav-links" id="tv-chat-link">TV Chat</a></li>
                <li><a href="#" class="nav-links" id="movie-chat-link">Movie Chat</a></li>
                <li><a href="#" class="nav-links" id="sports-chat-link">Sports Chat</a></li>
                <li><a href="#" class="nav-links" id="edit-profile-link">Edit Profile</a></li>
            </ul>
        </nav>
        <div class="row" id="search-bar-div-profile">
            <div class="input-field col m6 offset-m3" id="post-bar-col">
                <input placeholder="Say Something" name="post" type="text" id="post-input"/>
                <input class="btn" type="submit" value="POST" id="post-button"/>
            </div>
            <div class="col m3" id="logout-button-col">
                <a href="/logout" class="waves-effect waves-light btn" id="logout-button"><i class="material-icons left">cloud_off</i>Logout</a>
            </div>
        </div>

        <div class="row" id="feed-row">;
                <div class="col m6 offset-m3" id="feed">

        {{#each feed}}
         <div class="card-panel" id="post">
                     <div class="card-content">
                        <a href="#" id="postername">{{username}}</a><span id="says"> says:</span>
                        <br>
                        <br>
                        <span id="post-body">{{message}}</span>
                        <br><br>
                        <a href="#" id="reply">REPLY</a>
                    </div>
                </div>
        {{/each}}

            </div>    
            </div>

        <div class="row" id="profile-page-content">
            <div class="col m3" id="discord-app-col">
                <iframe src="https://discordapp.com/widget?id=406842588239495168&theme=dark" width="350" height="500" allowtransparency="true" frameborder="0" id="discord-widget"></iframe>
            </div>
            <div class="col m6" id="profile-feed-col">

            </div>
            <div class="col m3" id="friends-col">
                <div class="card" id="friends-card">
                    <div class="card-content" id="friends-content">
                        <span class="card-title" id="friends-title">You have <a href="#" id="friends-count">4</a> friends</span>
                        <div class="friend">
                            <img src="https://statusandphoto.weebly.com/uploads/6/0/1/5/60158603/8347592_orig.png" id="friend-img"/>
                            <div class="friend-name">
                                <a href="#" class="friendname">@friendname</a>
                            </div>
                        </div>
                        <div class="friend">
                             <img src="http://www.trickscity.com/wp-content/uploads/2016/07/Cool-And-Stylish-Profile-Pictures-For-Facebook-For-Girls-17-1024x993.jpg" id="friend-img"/>
                            <div class="friend-name">
                                <a href="#" class="friendname">@friendname</a>
                            </div>
                        </div>
                        <div class="friend">
                            <img src="http://static2.businessinsider.com/image/5899ffcf6e09a897008b5c04-1200/.jpg" id="friend-img"/>
                            <div class="friend-name">
                                <a href="#" class="friendname">@friendname194503849</a>
                            </div>
                        </div>
                        <div class="friend">
                            <img src="https://cdn.imaginetricks.com/wp-content/uploads/2017/08/Wonderful-Cute-Girls-Profile-Picture.jpg" id="friend-img"/>
                            <div class="friend-name">
                                <a href="#" class="friendname">@friendname</a>
                            </div>
                        </div>
                    </div>
                </div>
                
            </div>
            
        </div>
    </div>

